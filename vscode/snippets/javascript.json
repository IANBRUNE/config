      "import { action } from '@storybook/addon-actions'",
      "",
      "import Component from './Component'",
      "",
      "storiesOf('Component', module).add('Default', () => (",
      "",
      " <View",
      "   style={{",
      "   flex: 1,",
      "   flexDirection: 'row',",
      "   justifyContent: 'center',",
      "   alignItems: 'flex-start'",
      "   }}",
      " >",
      "   <Component />",
      " </View>",
      " ))"
    ],

    "description": "Default storybook imports"
  }
}
